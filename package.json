{
  "author": "Koichi Suematsu",
  "license": "MIT",
  "name": "ibs_th1",
  "description": "Library to process data broadcasted from IBS-TH1 / IBS-TH1 mini / IBS-TH1 Plus.",
  "version": "1.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/pascaljp/ibs_th1.git"
  },
  "bugs": {
    "url": "https://github.com/pascaljp/ibs_th1/issues"
  },
  "keywords": [
    "inkbird",
    "ibs-th1"
  ],
  "main": "dist/ibs_th1.js",
  "types": "dist/ibs_th1.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --outDir dist --lib es6 --downlevelIteration --esModuleInterop --noUnusedLocals --noUnusedParameters --noImplicitReturns --noFallthroughCasesInSwitch --types --noImplicitThis --alwaysStrict --strictPropertyInitialization --strictBindCallApply --strictFunctionTypes --strictNullChecks --noImplicitAny ibs_th1.ts",
    "test": "ts-node tests/sample.ts"
  },
  "engines": {
    "node": ">=6"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "dependencies": {
    "@abandonware/noble": "^1.9.2-5",
    "log4js": "^6.6.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.35"
  }
}
